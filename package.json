  },
  "dependencies": {
    "@vercel/ncc": "^0.38"
  },
  "peerDependencies": {
    "@stencil/core": "^4.0"
  },
  "devDependencies": {
    "@stencil/core": "^4.0",
    "@types/node": "^20.7",
    "prettier": "^3.0.0",
    "typescript": "^5.1"
  }
}
