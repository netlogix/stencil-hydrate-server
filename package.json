    "url": "https://github.com/netlogix/stencil-hydrate-server.git"
  },
  "dependencies": {
    "@vercel/ncc": "^0.36"
  },
  "peerDependencies": {
    "@stencil/core": "^3.3"
  },
  "devDependencies": {
    "@stencil/core": "^3.3",
    "@types/node": "^20.3",
    "prettier": "^2.8.8",
    "typescript": "^5.0"
  }
}
